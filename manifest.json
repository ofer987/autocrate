{
  "name": "Autocrate",
  "description": "Switch between AEM servers and pages (Editor, Preview, Sites, Editor, Package Manager, and more)",
  "version": "0.1.0",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Autocrate",
    "default_popup": "main.html"
  },
  "commands": {
    "select": {
      "suggested_key": {
        "default": "Alt+K",
        "mac": "Alt+K"
      },
      "description": "Change between servers/pages"
    },
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+1",
        "mac": "Alt+1"
      },
      "description": "Change pages"
    }
},
  "manifest_version": 2
}
