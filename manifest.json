{
  "name": "Autocrate",
  "description": "Switch between AEM servers and pages (Editor, Preview, Sites, Editor, Package Manager, and more)",
  "version": "0.1.0",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Autocrate",
    "default_popup": "main.html"
  },
  "commands": {
    "select-page": {
      "suggested_key": {
        "default": "Alt+J",
        "mac": "Alt+K"
      },
      "description": "Change page"
    }
  },
  "manifest_version": 2
}
